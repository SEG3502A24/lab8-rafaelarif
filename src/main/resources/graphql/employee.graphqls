type Query 
{
    employees: [Employee]
    employeeById(id: ID!): Employee
}

type Employee 
{
    id: ID!
    name: String!
    dateOfBirth: String
    city: String
    salary: Float
    gender: String
    email: String
}

input CreateEmployeeInput 
{
    name: String!
    dateOfBirth: String
    city: String
    salary: Float
    gender: String
    email: String
}

type Mutation 
{
    createEmployee(input: CreateEmployeeInput): Employee
}